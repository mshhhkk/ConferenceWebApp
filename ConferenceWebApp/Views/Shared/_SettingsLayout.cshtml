<!DOCTYPE html>
<html lang="ru">
<head>
    <meta charset="utf-8" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.1/font/bootstrap-icons.css">
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@500&display=swap" rel="stylesheet">
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="stylesheet" href="~/css/global.css" />
    <link rel="stylesheet" href="~/css/footerPA.css" />
    <link rel="stylesheet" href="~/css/header.css" />
    <link rel="stylesheet" href="~/css/personalaccount.css" />
    <link rel="stylesheet" href="~/css/reports.css" />
    <link rel="stylesheet" href="~/css/settings.css" />
</head>
<body>
        @await Html.PartialAsync("_HeaderPA")
        <div class="up-gradient-pa-bg"></div>
            <div class="personal-account-layout">
                <div class="personal-account-main">
                    <div class="settings-title">Настройки</div>
                    @await Html.PartialAsync("_SettingsTabs")
                        <div class="settings-content-box">
                        @RenderBody()
                    </div>
                </div>
            </div>
         <div class="down-gradient-bg"></div>
        @await Html.PartialAsync("_Footer")
    @RenderSection("Scripts", required: false)
</body>
</html>
